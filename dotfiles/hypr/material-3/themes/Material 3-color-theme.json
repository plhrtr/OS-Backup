        "input.background": "#261d1f",
        "input.border": "#9e8c8f",
        "input.foreground": "#efdee0",
        "input.placeholderForeground": "#efdee080",
        "inputOption.activeBackground": "#e4bdc4",
        "inputOption.activeForeground": "#43292f",

