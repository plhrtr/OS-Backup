        "tab.activeBackground": "#191113",
        "tab.inactiveBackground": "#261d1f",
        "tab.hoverBackground": "#261d1f",
        "editorGroupHeader.tabsBackground": "#261d1f",

        // Widgets
        "editorWidget.background": "#261d1f",
        "editorWidget.foreground": "#efdee0",
        "pickerGroup.foreground": "#ffb1c2",
        "debugToolBar.background": "#261d1f",

