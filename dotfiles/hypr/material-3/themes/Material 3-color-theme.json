        "button.background": "#ffb1c2",
        "button.foreground": "#551d2d",
        "button.hoverBackground": "#ffb1c2F0",
        "button.secondaryBackground": "#3c3234",
        "button.secondaryForeground": "#efdee0",
        "button.secondaryHoverBackground": "#3c3234F0",

